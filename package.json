{
  "name": "jgroup-app-messages",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/jgroup-app-messages/jgroup-app-messages.esm.js",
  "exports": {
    ".": {
      "import": "./dist/jgroup-app-messages/jgroup-app-messages.esm.js",
      "require": "./dist/jgroup-app-messages/jgroup-app-messages.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@stencil/eslint-plugin": "^1.0.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^3.5.3",
    "puppeteer": "^24.3.0",
    "stencil-tailwind-plugin": "^2.0.5",
    "tailwindcss": "^4.1.4"
  },
  "license": "MIT",
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.6.2"
  }
}
